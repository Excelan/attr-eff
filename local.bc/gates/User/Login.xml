<?xml version="1.0" encoding="UTF-8"?>

<gate ns="User" name="Login">

    <role>

    </role>

    <request>

        <field name="user" type="integer" required="no"/>

        <field name="email" type="string" required="yes"/>
        <field name="password" type="string" required="yes"/>

    </request>


    <response>

        <field name="status" type="integer" required="yes"/>
        <field name="redirect" type="string" required="yes"/>

    </response>

</gate>
